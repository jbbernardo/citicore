{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.3",
	    "silverstripe/cms": "3.7.x-dev",
		"silverstripe/framework": "3.7.x-dev",
		"silverstripe/reports": "3.7.x-dev",
		"silverstripe/siteconfig": "3.7.x-dev",
		"unclecheese/mock-dataobjects": "dev-master",
		"undefinedoffset/sortablegridfield": "^1.0",
		"bummzack/sortablefile": "1.0.*@dev",
		"gargron/fileupload": "~1.1",
		"unclecheese/betterbuttons": "1.3.*",
		"phpmailer/phpmailer": "^6.0"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7@stable"
	},
	"extra": {
		"branch-alias": {
			"3.x-dev": "3.6.x-dev"
		}
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
